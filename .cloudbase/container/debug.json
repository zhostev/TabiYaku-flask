{
  "containers": [
    {
      "name": "wxcloudrun-flask",
      "containerId": "8a18e8291d07de04023f770fa4e0376c4749423a34711bd05c8eb00bc9acd1d9",
      "domain": "",
      "ip": "",
      "mode": "compose"
    }
  ],
  "config": {
    "wxcloudrun-flask": {
      "containerPort": 80,
      "minNum": 0,
      "maxNum": 5,
      "cpu": 1,
      "mem": 2,
      "policyType": "cpu",
      "policyThreshold": 60,
      "policyDetails": [
        {
          "PolicyType": "cpu",
          "PolicyThreshold": 60
        },
        {
          "PolicyType": "mem",
          "PolicyThreshold": 60
        }
      ],
      "envParams": {
        "MYSQL_USERNAME": "",
        "MYSQL_PASSWORD": "",
        "MYSQL_ADDRESS": ""
      },
      "customLogs": "stdout",
      "initialDelaySeconds": 2,
      "dataBaseName": "flask_demo",
      "executeSQLs": [
        "CREATE DATABASE IF NOT EXISTS flask_demo;",
        "USE flask_demo;",
        "CREATE TABLE IF NOT EXISTS `Counters` (`id` int(11) NOT NULL AUTO_INCREMENT, `count` int(11) NOT NULL DEFAULT 1, `createdAt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP, `updatedAt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP, PRIMARY KEY (`id`)) ENGINE = InnoDB DEFAULT CHARSET = utf8;"
      ]
    }
  }
}